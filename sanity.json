{
  "root": true,
  "project": {
    "name": "landing-page-builder"
  },
  "api": {
    "projectId": "9itgab5x",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "json-input",
    "tabs",
    "color-list",
    "visual-options",
    "@webriq-pagebuilder/sanity-plugin-webriq-forms",
    "@webriq-pagebuilder/sanity-plugin-schema-default",
    "@webriq-pagebuilder/sanity-plugin-schema-commerce",
    "@webriq-pagebuilder/sanity-plugin-schema-blog",
    "@webriq-pagebuilder/sanity-plugin-webriq-payments",
    "@webriq-pagebuilder/sanity-plugin-webriq-blog",
    "media",
    "dashboard-widget-netlify",
    "dashboard-widget-document-list",
    "google-analytics",
    "@sanity/production-preview"
  ],
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "implements": "part:@sanity/production-preview/resolve-production-url",
      "path": "./src/resolvePreviewUrl.js"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./src/deskStructure/index.js"
    },
    {
      "implements": "part:@sanity/base/brand-logo",
      "path": "./src/brand/logo.js"
    },
    {
      "implements": "part:@sanity/base/theme/variables/override-style",
      "path": "./src/brand/styles/variable.css"
    },
    {
      "implements": "part:@sanity/form-builder/input/image/asset-sources",
      "path": "./src/assetSources.js"
    },
    {
      "implements": "part:@sanity/base/document-badges/resolver",
      "path": "./src/resolveDocumentBadges.js"
    },
    {
      "implements": "part:@sanity/base/document-actions/resolver",
      "path": "./src/documentActions/index.js"
    }
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision",
        "parts-list",
        "@andre-brdoch/sanity-plugin-schema-inspector"
      ]
    }
  }
}
